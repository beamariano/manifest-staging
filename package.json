    "react-player": "^3.3.1",
    "react-rough": "^2.3.0",
    "rough-react-wrapper": "^1.0.2",
    "roughjs": "^4.6.6",
    "tailwindcss-motion": "^1.1.1",
    "wired-elements": "^3.0.0-rc.6"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.2",
    "tailwindcss": "^4",
    "typescript": "^5",
